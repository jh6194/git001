<script src="//t1.daumcdn.net/mapjsapi/bundle/postcode/prod/postcode.v2.js"></script>
<script>

	/* 우편번호 */
	/* 우편번호 */

	$(function() {
		$("#insertPost").on("click", function() {
			new daum.Postcode({
				oncomplete : function(data) {
					$("#insertPostcode").val(data.zonecode);
					$("#insertAddress").val(data.address);
					$("#insertAddress2").focus();
	         	}
	      	}).open();
	   	});
		
		/* 중복확인 */
		/* 중복확인 */
		
		var i = "";
			$("#doubleBtn").on("click", function() {	// 중복확인 버튼 클릭 이벤트
				$.ajax({
					url:"${pageContext.request.contextPath}/double" 
					,type:"get" 
					,dataType:"text" 
					,data:{"insertId":$("#insertId").val()}	// 클릭시 사용자가 입력한 아이디 
					,success:function(data) {
							i = $("#insertId").val(); // 중복확인 후 다른 아이디로의 변경을 체크하기 위한 변수
							if(data == 1) {	// DB select 결과 동일한 ID가 있다면
							$("#double").text("사용이 불가능한 아이디입니다.");
							$("#double").css("color", "red");
						} else {			// DB select 결과 동일한 ID가 없다면
							$("#double").text("사용가능한 아이디입니다.");
							$("#double").css("color", "blue");
							$("#doubleCheck").val(1);
						}
					},error:function(xhr,text,Status,errorThrown) {
						$("#double").html(textStauts + "(HTTP-"+xht.status+"/"+errorThreown);
					}
				});
			});
			
		/* 이메일 인증 */
		/* 이메일 인증 */
				
		var j = "";
 			$("#iENsend").on("click", function() {
				$.ajax({
					url:"${pageContext.request.contextPath}/mail.we"
					,type:"post"
					,dataType:"text"
					,data:{ "email":$("#insertEmail").val() }
					,success:function(data) {
						j = $("#insertEmail").val();	// 인증 후 이메일 변경을 체크하기 위한 변수
						alert("인증번호가 발송되었습니다.");	
						$("#iENsend").hide();			// 인증요청 버튼 숨기기
						$("#iENreceive").show();		// 인증 버튼 보이기
						$("#iENresend").show();			// 재전송 버튼 보이기
						$("#iEN").val(data);			// 인증번호 비교를 위한 변수
					},error: function(xhr,text,Status,errorThrown) {
						$("#insertEmailNum").html(textStauts + "(HTTP-"+xht.status+"/"+errorThreown);
					}
				});
			});
				
		 	$("#iENresend").on("click", function() {
				$.ajax({
					url:"${pageContext.request.contextPath}/mail.we"
					,type:"post"
					,dataType:"text"
					,data:{ "email":$("#insertEmail").val() }
					,success:function(data) {
						j = $("#insertEmail").val();
						alert("인증번호가 재발송되었습니다.");
						$("#insertEmailNum").removeAttr("readonly", "readonly");
						$("#iEN").val(data);
						$("#iENcheck").val(-1);
					},error: function(xhr,text,Status,errorThrown) {
						$("#insertEmailNum").html(textStauts + "(HTTP-"+xht.status+"/"+errorThreown);
					}
				});
			});
			
			$("#iENreceive").on("click", function() {
				if ($("#iEN").val() == $("#insertEmailNum").val()) {
					alert("인증에 성공하였습니다.");
					$("#insertEmailNum").attr("readonly", "readonly");
					$("#iENreceive").attr("disabled", "disabled");
					$("#iENcheck").val(1);
				} else {
					alert("인증에 실패하였습니다.");
				}
			});
			
			/* 전체선택-해제 */
			/* 전체선택-해제 */
			
			$("#checkboxAll").click(function() {	// 전체동의 체크박스 클릭 이벤트
				if($("#checkboxAll").is(":checked")) $("input[name=checkbox]").prop("checked", true);
				else $("input[name=checkbox]").prop("checked", false);
			});

			$("input[name=checkbox]").click(function() {	// 개별 체크박스 클릭 이벤트
				var total = $("input[name=checkbox]").length;
				var checked = $("input[name=checkbox]:checked").length;

				if(total != checked) $("#checkboxAll").prop("checked", false);	// 전체동의 해제
				else $("#checkboxAll").prop("checked", true); // 전체동의 체크
			});
			
			/* 빈칸검사 */
			/* 빈칸검사 */
		
			$("#insertForm").on("submit", function() {
				 if($("#insertPass").val() != $("#insertPassCheck").val()) {
					alert('비밀번호가 일치하지 않습니다.');
					$("#insertPassCheck").focus();
					return false;
				} else if($("#insertPostcode").val() == "") {
					alert('우편번호를 검색해주세요.');
					$("#insertPost").focus();
					return false;
				} else if(!$('#checkbox1').is(':checked')) {
					alert('이용약관에 동의하지 않았습니다.');
					return false;
				} else if(!$('#checkbox2').is(':checked')) {
					alert('개인정보 취급방침에 동의하지 않았습니다.');
					return false;
				} else if($("#doubleCheck").val() == -1 || i != $("#insertId").val()) {
					alert('아이디 중복검사를 해주세요.');
					return false;
				} else if($("#iENcheck").val() == -1 || j != $("#insertEmail").val()) {
					alert('이메일 인증을 진행해주세요.');
					return false;
				}
			});
		});
</script>
	
