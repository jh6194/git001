<script>
	/* 공백제거 */
	/* 공백제거 */
		
	function noSpaceForm(obj) {	// 정규식을 이용한 공백제거 
	    var str_space = /\s/;
	    if(str_space.exec(obj.value)) {	// exec() : 정규식 패턴에 맞는 문자열 탐색을 수행하는 메소드
	        obj.value = obj.value.replace(' ','');	// replace() : 패턴에 일치하는 부분을 원하는 문자열로 치환하는 메소드 
	        return false;
	    }
	}
</script>

